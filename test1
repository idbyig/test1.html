function isVisible(element) {
    if(element && typeof element == 'string'){
		element = document.querySelectorAll(element);
	}
	if (element instanceof NodeList) {
		element = element[0];
	}
	if(element instanceof Element) {
		return ((element.offsetWidth > 0 && element.offsetHeight > 0) || element['getClientRects']().length > 0);
	}
	return false;
}

function doUpload() {
    if (typeof ClickTaleUploadPageNow === 'function' && ClickTaleIsRecording()) {
        ClickTaleUploadPageNow();
    }
}

function isReadyToRecord() {
    if (typeof ClickTaleUploadPageNow === 'function' && ClickTaleIsRecording()) {
		if(/account.godaddy.com\/products(?:-alpha)?\//i.test(location.href)){
			return !isVisible('img.loader');
		}
		else if(/account.godaddy.com\/overview\//i.test(location.href)){
			return !isVisible('.ux-spinner-primary');
		}
		else if(/myh.godaddy.com/i.test(location.host)){
			if(/\/hosting\/mwp\/index/i.test(location.hash)){
				return isVisible('a[data-eid="hosting.myh.mwp.site.settings.click"]');
			}
			if(/\/hosting\/mwp\/site/i.test(location.hash)){
				return !isVisible('.ux-spinner') && isVisible('.mwp-site-security-row-cell-right #noSslToggle');
			}
		}
        else if ((docHref.indexOf("bulk-domain-search.aspx") > -1) || (docHref.indexOf("domains/domain-transfer.aspx") > -1)) {
            return ((window['jQuery']) && (!jQuery('#bulk_pricing_json_contentspinner:visible').length) && (jQuery('#dialog_bulk').length));
        }
        else if (docHref.indexOf("domains/searchresults.aspx") > -1) {
            return ((window['jQuery']) && (!jQuery('.spinner_div:visible').length) && (jQuery('#stripMall .result').length));
        }
        else if (docHref.indexOf("domains/gtld-registration.aspx") > -1) {
            return ((window['jQuery']) && (jQuery('#eligibility-form:visible').length));
        }
        else if (document.location.pathname.toLowerCase().indexOf('/upp/') > -1) {
            return (document.querySelectorAll('.app')[0] && document.querySelectorAll('.spinner').length === 0);
        }
        else if (docHref.indexOf('account.godaddy.com/billing') > -1 || docHref.indexOf('account.godaddy.com/renewals') > -1) {
            return (document.querySelectorAll('.spinner').length === 0);
        } else if (docHref.indexOf('dcc.godaddy.com/manage/') > -1 ) {
            return (document.querySelectorAll('.ng-scope.sf-msgoverlay .sf-msg-overlay-status').length === 0 && document.querySelectorAll('.ng-isolate-scope.sf-msgoverlay').length === 0 && isVisible('[ng-show="viewLoading"]') === false);
        }
        return true;
    }
    return false;
}

function doOnlyWhen(toDoHandler, toCheckHandler, interval, times, failHandler) {
    if ((!toDoHandler) || (!toCheckHandler)) return;
    if (typeof interval == "undefined") interval = 1000;
    if (typeof times == "undefined") times = 20;
    if (--times < 0) {
        if (typeof failHandler === 'function') {
            failHandler();
        }
        return;
    }
    if (toCheckHandler()) {
        toDoHandler();
        return;
    }
    setTimeout(function() {
        doOnlyWhen(toDoHandler, toCheckHandler, interval, times, failHandler);
    }, interval);
}
if (typeof ClickTaleUploadPage === 'function' && window.ClickTaleSettings.PTC.UseTransport) {

    ClickTaleDelayUploadPage();

    doOnlyWhen(doUpload, isReadyToRecord, 500, 20, doUpload);

    var initFuncs = window.ClickTaleSettings.PTC.InitFuncs;
    for (var i = 0, initLen = initFuncs.length; i < initLen; i++) {
        if (typeof initFuncs[i] === 'function') {
            initFuncs[i]();
        }
    }
}

	// End of user-defined header code (PreInitialize)
    
	
	window.ClickTaleIncludedOnDOMReady=true;
	
	ClickTaleGlobal.init.isAllowed && ClickTale(PID, Ratio, PartitionPrefix, SubsId);
	
	if((typeof ClickTalePrevOnReady == 'function') && (ClickTaleOnReady.toString() != ClickTalePrevOnReady.toString()))
	{
    	ClickTalePrevOnReady();
	}
	
	
	// Start of user-defined footer code
	
	// End of user-defined footer code
	
}; 
